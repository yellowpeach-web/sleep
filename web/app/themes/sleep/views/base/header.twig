{% set home_url = fn('home_url') %}

<!DOCTYPE html>
<html {{ fn('language_attributes') }}>

<head>
	<meta charset="{{ site.charset }}">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="mobile-web-app-capable" content="yes">
	<meta name="apple-sm-web-app-capable" content="yes">
	<meta name="apple-sm-web-app-title" content="{{ site.name }} - {{ site.description }}">
	<link rel="profile" href="http://gmpg.org/xfn/11">
	<link rel="pingback" href="{{ site.pingback_url }}">
	{# favicons #}
	{% set template_directory_uri = fn('get_template_directory_uri') ~ '/resources/images/favicons/'%}
	<link rel="apple-touch-icon-precomposed" sizes="57x57"
		href="{{template_directory_uri}}apple-touch-icon-57x57.png" />
	<link rel="apple-touch-icon-precomposed" sizes="114x114"
		href="{{template_directory_uri}}apple-touch-icon-114x114.png" />
	<link rel="apple-touch-icon-precomposed" sizes="72x72"
		href="{{template_directory_uri}}apple-touch-icon-72x72.png" />
	<link rel="apple-touch-icon-precomposed" sizes="144x144"
		href="{{template_directory_uri}}apple-touch-icon-144x144.png" />
	<link rel="apple-touch-icon-precomposed" sizes="60x60"
		href="{{template_directory_uri}}apple-touch-icon-60x60.png" />
	<link rel="apple-touch-icon-precomposed" sizes="120x120"
		href="{{template_directory_uri}}apple-touch-icon-120x120.png" />
	<link rel="apple-touch-icon-precomposed" sizes="76x76"
		href="{{template_directory_uri}}apple-touch-icon-76x76.png" />
	<link rel="apple-touch-icon-precomposed" sizes="152x152"
		href="{{template_directory_uri}}apple-touch-icon-152x152.png" />
	<link rel="icon" type="image/png" href="{{template_directory_uri}}favicon-196x196.png" sizes="196x196" />
	<link rel="icon" type="image/png" href="{{template_directory_uri}}favicon-96x96.png" sizes="96x96" />
	<link rel="icon" type="image/png" href="{{template_directory_uri}}favicon-32x32.png" sizes="32x32" />
	<link rel="icon" type="image/png" href="{{template_directory_uri}}favicon-16x16.png" sizes="16x16" />
	<link rel="icon" type="image/png" href="{{template_directory_uri}}favicon-128.png" sizes="128x128" />
	{# favicons #}

	<script>(function (w, d, s, l, i) {
			w[l] = w[l] || []; w[l].push({
				'gtm.start':
					new Date().getTime(), event: 'gtm.js'
			}); var f = d.getElementsByTagName(s)[0],
				j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
					'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
		})(window, document, 'script', 'dataLayer', 'GTM-MZ2SQKDF');</script>

	{{ fn('wp_head') }}
	{% if options.head_scripts %}
	{{ options.head_scripts }}
	{% endif %}
</head>

<body id="" class="yp-frontend {{ body_class }}">

	<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-MZ2SQKDF" height="0" width="0"
			style="display:none;visibility:hidden"></iframe></noscript>

	<div class="site" id="page">

		{# The Navbar Area #}
		<div id="wrapper-navbar" itemscope itemtype="http://schema.org/WebSite"
			class="sticky top-0 z-20 flex-flex-col {{general_settings.header_color}}">
			<div class="bg-background-primary-bg-fill">
				{# Announcement bar #}
				{% if general_settings.announcement_bar %}
				<a href="{{general_settings.announcement_bar.url}}"
					class="no-link-style announcement-bar general-transition block w-full">
					<div class="bg-button-primary-bg-fill-default py-1.5">
						<div class="yp-container flex justify-center">
							<p class="!m-0 overline-large text-button-primary-text-fill-default">
								{{general_settings.announcement_bar.title}}
							</p>
						</div>
					</div>
				</a>
				{% endif %}
				<header class="">
					<div class="yp-container py-5 xl:py-0 flex items-center justify-between w-full px-0">
						<a href="{{home_url}}" title="Take me to the homepage"
							class="no-link-style w-auto flex grow-0 shrink-0 h-10" aria-label="Homepage"
							rel="home">
							{% include "resources/images/logo-no-dot.svg" %}
						</a>
						{% include "components/nav.twig" %}
					</div>
				</header>
				<!-- Menu mobile -->
				{% include "components/mobile-menu.twig" %}
			</div>
		</div>
		{# wrapper-navbar end #}



		<div id="main-content-inner">
			{# this id="main-content-inner" close on footer #}